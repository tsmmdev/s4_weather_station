              "datasource": {
                "type": "frser-sqlite-datasource",
                "uid": "${DS_SQLITE}"
              },
              "queryText": "SELECT\n  devices.id,\n  devices.description, \n  GROUP_CONCAT(CASE WHEN sensor_data.type = 'temperature' THEN sensor_data.value ELSE NULL END) AS temp,\n  GROUP_CONCAT(CASE WHEN sensor_data.type = 'pressure' THEN sensor_data.value ELSE NULL END) AS pressure,\n  -- GROUP_CONCAT(CASE WHEN sensor_data.type = 'dew' THEN sensor_data.value ELSE NULL END) AS dew,\n  -- GROUP_CONCAT(CASE WHEN sensor_data.type = 'gps' THEN sensor_data.value ELSE NULL END) AS gps,\n  -- GROUP_CONCAT(CASE WHEN sensor_data.type = 'baro' THEN sensor_data.value ELSE NULL END) AS baro,\n  GROUP_CONCAT(CASE WHEN sensor_data.type = 'humidity' THEN sensor_data.value ELSE NULL END) AS humidity\n  -- GROUP_CONCAT(CASE WHEN sensor_data.type = 'heat' THEN sensor_data.value ELSE NULL END) AS heat\nFROM devices \nJOIN sensor_data ON devices.id = sensor_data.device \nWHERE sensor_data.ts = (SELECT MAX(ts) FROM sensor_data s WHERE s.device = sensor_data.device AND s.type = sensor_data.type)\nGROUP BY devices.device;",
              "queryType": "table",
              "rawQueryText": "SELECT\n  devices.id,\n  devices.description, \n  GROUP_CONCAT(CASE WHEN sensor_data.type = 'temperature' THEN sensor_data.value ELSE NULL END) AS temp,\n  GROUP_CONCAT(CASE WHEN sensor_data.type = 'pressure' THEN sensor_data.value ELSE NULL END) AS pressure,\n  -- GROUP_CONCAT(CASE WHEN sensor_data.type = 'dew' THEN sensor_data.value ELSE NULL END) AS dew,\n  -- GROUP_CONCAT(CASE WHEN sensor_data.type = 'gps' THEN sensor_data.value ELSE NULL END) AS gps,\n  -- GROUP_CONCAT(CASE WHEN sensor_data.type = 'baro' THEN sensor_data.value ELSE NULL END) AS baro,\n  GROUP_CONCAT(CASE WHEN sensor_data.type = 'humidity' THEN sensor_data.value ELSE NULL END) AS humidity\n  -- GROUP_CONCAT(CASE WHEN sensor_data.type = 'heat' THEN sensor_data.value ELSE NULL END) AS heat\nFROM devices \nJOIN sensor_data ON devices.id = sensor_data.device \nWHERE sensor_data.ts = (SELECT MAX(ts) FROM sensor_data s WHERE s.device = sensor_data.device AND s.type = sensor_data.type)\nGROUP BY devices.device;",
              "refId": "A",
              "timeColumns": []
            }
          ],
          "type": "table"
        }
